{
  "expo": {
    "name": "whereToEat",
    "description": "Takes out the pain of making a decision about where to eat!",
    "slug": "whereToEat",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FEFEFE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ikenjoku.wheretoeat.wheretoeat"
    },
    "android": {
      "package": "com.ikenjoku.wheretoeat.wheretoeat"
    }
  }
}
